{
  "name": "web-scraper",
  "version": "1.0.0",
  "description": "Node.js Web Scraper projects",
  "main": "src/service.js",
  "scripts": {
    "clean": "rm -rf ./build/*",
    "babel-build": "babel src -d ./build ./src",
    "build": "npm run clean && npm run babel-build",
    "start": "npm run build && NODE_ENV=prod node ./build/service.js",
    "dev": "NODE_ENV=dev DEBUG=app,app:* nodemon --exec babel-node ./src/service.js",
    "test": "NODE_ENV=test mocha --require @babel/register --recursive --exit ./test/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lwx-amr/Web-Scraper"
  },
  "keywords": [
    "Node.js",
    "ES6",
    "Web Scraping"
  ],
  "author": "Amr Hussien, Yousef Ezzat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lwx-amr/Web-Scraper/issues"
  },
  "homepage": "https://github.com/lwx-amr/Web-Scraper#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "dotenv": "^9.0.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "helmet": "^4.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.0",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "@babel/register": "^7.13.16",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "core-js": "^3.20.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "regenerator-runtime": "^0.13.9"
  },
  "browserslist": [
    "last 3 chrome versions"
  ]
}
